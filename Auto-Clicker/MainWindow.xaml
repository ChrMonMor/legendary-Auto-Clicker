<Window x:Class="Auto_Clicker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Auto_Clicker"
        mc:Ignorable="d"
        Title="MainWindow" Height="150" Width="200">
    <Grid>
        <Button x:Name="button" Content="Start/Stop" Margin="0,73,0,0" Height="20" VerticalAlignment="Top" HorizontalAlignment="Center" Width="78" Click="button_Click"/>
        <TextBox x:Name="textBox" Margin="0,44,0,0" TextWrapping="Wrap" Text="100" TextAlignment="Right" Height="18" VerticalAlignment="Top" HorizontalAlignment="Center" Width="120" />
        <Label x:Name="label" Content="Intervals (ms)" HorizontalAlignment="Center" Margin="0,18,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>