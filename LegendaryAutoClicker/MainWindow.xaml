<Window x:Class="LegendaryAutoClicker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LegendaryAutoClicker"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="mainWindow" Initialized="mainWindow_Initialized" Loaded="mainWindow_Loaded">
        <CheckBox x:Name="checkBoxEnable" Content="Enable" HorizontalAlignment="Left" Margin="10,78,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxIntervals" HorizontalAlignment="Left" Margin="10,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" PreviewTextInput="textBoxIntervals_PreviewTextInput"/>
        <Button x:Name="buttonSet" Content="Set" HorizontalAlignment="Left" Margin="149,35,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label" Content="Intervals (ms)" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
